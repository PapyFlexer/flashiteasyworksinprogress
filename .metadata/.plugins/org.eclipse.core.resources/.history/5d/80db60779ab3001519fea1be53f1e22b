			private function init() : void
			{
				var today : Date = new Date();
				trace (today)
				//var validity : Number = today.getTime() + (3*30*24*3600*1000);// 3 mois
				var validity : Number = today.getTime() + (3*355*24*3600*1000);// 3 ans
				var validityDate : Date= new Date(validity);
				usersData = new XML (<access>
					<validitydate>{validityDate.fullYear+"/"+(validityDate.getMonth()+1).toString()+"/"+validityDate.date}</validitydate>
					<alloweddomains>{dmn}</alloweddomains>
					<users usrfirstname="Gilles" usrlastname="Roquefeuil" usrlogin="gilles" usrpassword="1009" usremail="gilles.roquefeuil@sfr.fr" usradminlevel="0" usrlanguage="fr" />
					<users usrfirstname="Didier" usrlastname="Reyt" usrlogin="didier" usrpassword="1984" usremail="dr@flashiteasy.com" usradminlevel="0" usrlanguage="fr" />
					<users usrfirstname="Dany" usrlastname="Siriphol" usrlogin="dany" usrpassword="1987" usremail="ds@flashiteasy.com" usradminlevel="0" usrlanguage="fr" />
					<users usrfirstname="Carine" usrlastname="Magda" usrlogin="carine" usrpassword="1980" usremail="cm@flashiteasy.com" usradminlevel="0" usrlanguage="fr" />
					<users usrfirstname="Local" usrlastname="User" usrlogin="local" usrpassword="local" usremail="contact@flashiteasy.com" usradminlevel="0" usrlanguage="fr" />
					<users usrfirstname="Sune" usrlastname="Reyt" usrlogin="sunray" usrpassword="sune1985" usremail="sune.reyt@clubcase.fr" usradminlevel="0" usrlanguage="fr" />
					</access>);
					trace (usersData.toString());
				this.validitydate.selectedDate=validityDate;		
			}

			private function generate( e : MouseEvent ) : void
			{
				//var s : String = Base64.encrypt( domain.text);
				//trace (s); 
			}

			private function saveAccess( e : MouseEvent ) : void
			{
				accessData.domain.@url = dmn;
				trace ("saving... "+accessData.toString());
				var data : ByteArray = new ByteArray();
				data.writeUTFBytes( accessData ); // xmlData is original XML string
				data.compress();
				// feable encryption (rotation by 128)
				var j : int = data.length;
				while ( j-- )
				{
					data[ j ] += 128;
				}
				// save access in binary encrypted form
				new FileReference().save( data, "access.xml" );
			}
			private function testFileRef():void
			{
				fileRef.data.readBytes(new ByteArray(),0,0);
			}

			private function saveAdminUsers( e : MouseEvent ) : void
			{
				usersData.alloweddomains[0] = dmn;
				var data : ByteArray = new ByteArray();
				data.writeUTFBytes( usersData ); // xmlData is original XML string
				data.compress();
				// feable encryption (rotation by 128)
				var j : int = data.length;
				while ( j-- )
				{
					data[ j ] += 128;
				}
				// save access in binary encrypted form
				new FileReference().save( data, "users.xml" );
			}

			private function loadAdminUsers( e : MouseEvent ) : void
			{
				var pathToXMLFile : String = pathToUser.text != "" ? pathToUser.text : defaultPathToUsersXml;
				trace( "loading users file..." );
				var ldr : URLLoader = new URLLoader();
				ldr.dataFormat = URLLoaderDataFormat.BINARY;
				ldr.addEventListener( Event.COMPLETE, onAdminUsersLoaded );
				ldr.addEventListener( IOErrorEvent.IO_ERROR, onAdminUsersLoaded );
				ldr.load( new URLRequest( pathToXMLFile ));
			}

			private function loadAccessFile( e : MouseEvent ) : void
			{
				var pathToXMLAccessFile : String = pathToAccess.text != "" ? pathToAccess.text : defaultPathToAccessXml;
				trace( "loading access file..."+pathToXMLAccessFile+" ///" );
				var ldr : URLLoader = new URLLoader();
				ldr.dataFormat = URLLoaderDataFormat.BINARY;
				ldr.addEventListener( Event.COMPLETE, onAccessLoaded );
				ldr.addEventListener( IOErrorEvent.IO_ERROR, onAccessLoaded );
				ldr.load( new URLRequest( pathToXMLAccessFile ));
			}

			private function onAdminUsersLoaded( e : Event ) : void
			{
				
				if ( e.type == Event.COMPLETE )
				{
					var data : * = URLLoader( e.target ).data;
					if ( data is ByteArray )
					{
						try
						{
							var i : uint = data.length;
							while ( i-- )
							{
								data[ i ] += 128
							}
							ByteArray( data ).uncompress();
						}
						catch ( e : Error )
						{
						}
					}
					trace (data);
					usersData = XML( data );
					domain.text = usersData.alloweddomains[0].toString();
					//pathToUser.text = usersData.
					this.validitydate.selectedDate = new Date(usersData.validitydate[0].toString());
					refreshGrid();

				}
			}
			private function onAccessLoaded( e : Event ) : void
			{
				trace ("callback access loaded"+URLLoader( e.target ).data);
				if ( e.type == Event.COMPLETE )
				{
					var data : * = URLLoader( e.target ).data;
					if ( data is ByteArray )
					{
						try
						{
							var i : uint = data.length;
							while ( i-- )
							{
								data[ i ] += 128
							}
							ByteArray( data ).uncompress();
						}
						catch ( e : Error )
						{
						}
					}
					trace (data);
					accessData = XML( data );
					dmn = accessData.domain.@url;
					domain.text = dmn
				}
			}
			
			private function refreshGrid() : void
			{
				dp = usersData.users as XMLList;
			}
			
			private function editUser(e:MouseEvent):void
			{
				var i : uint = usersList.selectedIndex;
				
				pop = new user;
				trace ("==>"+usersData.users[i].@usrfirstname);
				
				pop.currentUserState = "edit";
				PopUpManager.addPopUp(pop, this, true);
				PopUpManager.centerPopUp(pop);
				pop.usrfirstname.text = usersData.users[i].@usrfirstname;
				pop.usrlastname.text = usersData.users[i].@usrlastname;
				pop.usremail.text = usersData.users[i].@usremail;
				pop.usrlogin.text = usersData.users[i].@usrlogin;
				pop.usrpassword.text = usersData.users[i].@usrpassword;
				pop.usradminlevel.selectedIndex = pop.selectAdminLevelCombo( Number( usersData.users[i].@adminlevel ) );
				pop.usrlanguage.selectedIndex = pop.selectAdminLanguageCombo(usersData.users[i].@usrlanguage);
				
			}
			
			private function addAdminUser(e:MouseEvent):void
			{
				pop  =  new user;
				pop.currentUserState = "add";
				PopUpManager.addPopUp(pop,this,true);
				PopUpManager.centerPopUp(pop);
			}
			
			public function validateNewUser( name : String,
											 firstname : String,
											 email : String,
											 login : String,
											 password : String,
											 admlevel : uint = 5,
											 admlang : String = "fr_FR"
											) : void
			{
				var newUser : XML = new XML;
				newUser = <users usrfirstname={firstname} usrlastname={name} usremail={email} usrlogin={login} usrpassword={password} usradminlevel={admlevel} usrlanguage={admlang} />
				trace ("new user = " + newUser.toString());
				if (newUser.@usrlogin == "") newUser.@usrlogin = newUser.@usremail;
				if (newUser.@usrpassword == "") newUser.@usrpassword = "demo";
				 if (!usersData.contains(newUser)) usersData.appendChild(newUser);
				refreshGrid();
			}
			
			public function saveUser( name : String,
											 firstname : String,
											 email : String,
											 login : String,
											 password : String,
											 admlevel : uint = 5,
											 admlang : String = "fr_FR"
											) : void
			{
				
				usersData.users[usersList.selectedIndex] = <users usrfirstname={firstname} usrlastname={name} usremail={email} usrlogin={login} usrpassword={password} usradminlevel={admlevel} usrlanguage={admlang} />
				refreshGrid();
			}
			
			public function deleteUser(e:MouseEvent) : void
			{
				var i : uint = usersList.selectedIndex;
				delete usersData.users[i];
				refreshGrid();
			}
			
			private function onChange(e:ListEvent):void
			{
			}
			private function doChangeValidityDate():void
			{
				usersData.@validitydate = this.validitydate.selectedDate;
				trace ("new validity date : "+usersData.@validitydate);
			}
			
		]]>
	</mx:Script>
